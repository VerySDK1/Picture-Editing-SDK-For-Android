<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 滑竿背景 -->
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/pesdk_config_titlebar_bg" />

            <corners android:radius="1.5dp" />
        </shape>
    </item>
    <!-- 进度条 -->
    <item android:id="@android:id/progress">
        <!--        <clip>-->
        <shape>
            <solid android:color="@color/pesdk_materialcolorpicker__red" />

            <corners android:radius="1.5dp" />
        </shape>
        <!--        </clip>-->
    </item>
    <item android:id="@android:id/secondaryProgress">
        <!--            <clip>-->
        <shape>
            <solid android:color="@color/pesdk_main_color" />
            <corners android:radius="1.5dp" />
            <stroke
                android:width="1dp"
                android:color="@color/pesdk_main_color" />
        </shape>
        <!--            </clip>-->
    </item>

</layer-list>